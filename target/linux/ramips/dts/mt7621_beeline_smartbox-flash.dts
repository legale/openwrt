#include "mt7621.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "beeline,smartbox-flash", "mediatek,mt7621-soc";
	model = "Beeline SmartBox Flash";

	aliases {
		label-mac-device = &gmac0;

		led-boot = &led_sys_red;
		led-failsafe = &led_sys_red;
		led-running = &led_sys_green;
		led-upgrade = &led_sys_blue;
	};

	leds {
		compatible = "gpio-leds";

		led_sys_green: sys_green {
			label = "green:sys";
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
		};

		led_sys_red: sys_red {
			label = "red:sys";
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
		};

		led_sys_blue: sys_blue {
			label = "blue:sys";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
		};
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&gpio 3 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};
	
	ubi-concat {
		compatible = "mtd-concat";
		devices = <&ubipart0 &ubipart1>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;
			partition@0 {
				label = "ubi";
				reg = <0x0 0x5280000>;
			};
		};
	};
};

&nand {
	status = "okay";

	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		partition@0_all {
			label = "all";
			reg = <0x0 0x7f80000>;
			read-only;
		};

		bootloader: partition@0 {
			label = "bootloader";
			reg = <0x0 0x100000>;
		};

		ubootenv: partition@100000 {
			label = "config";
			reg = <0x100000 0x100000>;
		};

		factory: partition@200000 {
			label = "factory";
			reg = <0x200000 0x100000>;
		};

		partition@300000 { 
		/* This part. overlaps next one (stock layout). */
			label = "stock_kernel0";
			reg = <0x300000 0x2000000>;
		};

		partition@720000 {
			label = "stock_rootfs0";
			reg = <0x720000 0x1be0000>;
		};

		partition@2300000 { 
		/* This part. overlaps next one like before. */
			label = "stock_kernel1";
			reg = <0x2300000 0x2000000>;
		};

		partition@2720000 {
			label = "stock_rootfs1";
			reg = <0x2720000 0x1be0000>;
		};

		partition@4300000 {
			label = "stock_glbcfg";
			reg = <0x4300000 0x200000>;
		};

		partition@4500000 {
			label = "stock_board_data";
			reg = <0x4500000 0x100000>;
		};

		partition@4600000 {
			label = "stock_glbcfg2";
			reg = <0x4600000 0x200000>;
		};

		partition@4800000 {
			label = "stock_board_data2";
			reg = <0x4800000 0x100000>;
		};

		partition@4900000 {
			label = "stock_data";
			reg = <0x4900000 0x3680000>;
		};

		kernel: partition@2300000_owrt_kernel {
			label = "kernel";
			reg = <0x2300000 0x420000>;
		};

		ubipart0: partition@2720000_ubi0 {
			label = "ubipart0";
			reg = <0x2720000 0x1be0000>;
		};		
		
		ubipart1: partition@4600000_ubi1 {
			label = "ubipart1";
			reg = <0x4600000 0x3980000>;
		};		
	};
};


&pcie {
	status = "okay";
};

&pcie0 {
	wifi@0,0 {
		compatible = "mediatek,mt76";
		reg = <0x0000 0 0 0 0>;
		mediatek,mtd-eeprom = <&factory 0x0>;
		mtd-mac-address = <&factory 0x4>;
	};
};


&gmac0 {
 	mtd-mac-address = <&factory 0xe000>;
 };

&switch0 {
	ports {
		port@2 {
			status = "okay";
			label = "lan2";
		};

		port@3 {
			status = "okay";
			label = "lan1";
		};

		port@4 {
			status = "okay";
			label = "wan";
			mtd-mac-address = <&factory 0xe006>;
		};
	};
};

&state_default {
	gpio {
		groups = "jtag", "uart2", "uartlite3", "wdt";
		function = "gpio";
	};
};

