#!/bin/sh

[ -e /lib/firmware/$FIRMWARE ] && exit 0

. /lib/functions/caldata.sh

board=$(board_name)

case "$FIRMWARE" in
"ath11k/IPQ5018/hw1.0/cal-ahb-c000000.wifi.bin")
	case "$board" in
	linksys,mx2000|\
	linksys,mx5500)
		caldata_extract "0:ART" 0x440000 0x100000
		;;
	esac
	;;
"ath11k/QCN9024/hw1.0/cal-pci-0000:01:00.0.bin"|\
"ath11k/QCN9024/hw1.0/cal-pci-0001:01:00.0.bin")
	case "$board" in
	linksys,mx2000|\
	linksys,mx5500)
                caldata_extract "0:ART" 0x440000 0x100000
		;;
	esac
	;;
*)
	exit 1
	;;
esac
