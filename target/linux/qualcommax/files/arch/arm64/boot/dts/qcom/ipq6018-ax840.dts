/dts-v1/;

#define __IPQ_MEM_PROFILE_512_MB__

#include "ipq6018.dtsi"
#include "ipq6018-memory.dtsi"
#include "ipq6018-wifi.dtsi"
//#include "ipq6018-regulator-mp5496.dtsi"
#include "ipq6018-ess.dtsi"

#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>

/ {
	model = "Yuncore AX840";
	compatible = "yuncore,ax840", "qcom,ipq6018-cp03", "qcom,ipq6018";
	
	aliases {
		ethernet0 = "/soc/dp2";
		ethernet1 = "/soc/dp1";
		serial0 = &blsp1_uart3;
		led-boot = &led_system;
		led-failsafe = &led_system;
		led-running = &led_system;
		led-upgrade = &led_system;
	};

	chosen {
		stdout-path = "serial0:115200n8";
		bootargs-append = " ubi.block=0,rootfs root=/dev/ubiblock0_1 earlycon debug";
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&tlmm 19 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led_system: led_system {
			label = "system";
			color = "red";
			gpios = <&tlmm 32 GPIO_ACTIVE_HIGH>;
		};

		led_wlan2g {
			label = "wlan2g";
			color = "green";
			gpios = <&tlmm 37 GPIO_ACTIVE_HIGH>;
		};

		led_wlan5g {
			label = "wlan5g";
			color = "blue";
			gpios = <&tlmm 35 GPIO_ACTIVE_HIGH>;
		};
	};
};

&blsp1_uart3 {
	pinctrl-0 = <&serial_3_pins>;
	pinctrl-names = "default";
	status = "okay";
};

&tlmm {
	mdio_pins: mdio-pins {
		mdc {
			pins = "gpio64";
			function = "mdc";
			drive-strength = <8>;
			bias-pull-up;
		};

		mdio {
			pins = "gpio65";
			function = "mdio";
			drive-strength = <8>;
			bias-pull-up;
		};
	};

	led_enable {
		gpio-hog;
		output-high;
		gpios = <36 GPIO_ACTIVE_HIGH>;
		line-name = "enable-leds";
	};
};

&soc {
	dp1 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		reg = <0x0 0x3a001600 0x0 0x200>;
		qcom,mactype = <0>;
		qcom,id = <4>;
		qcom,phy-mdio-addr = <0x03>;
		phy-handle = <&rtl8211f_3>;
		phy-mode = "sgmii";
	};

	dp2 {
		device_type = "network";
		compatible = "qcom,nss-dp";
		reg = <0x0 0x3a001800 0x0 0x200>;
		qcom,mactype = <0>;
		qcom,id = <5>;
		qcom,phy-mdio-addr = <0x04>;
		phy-handle = <&rtl8211f_4>;
		phy-mode = "sgmii";
	};
};

&edma {
	status = "okay";
};

&mdio {
	status = "okay";
	pinctrl-0 = <&mdio_pins>;
	pinctrl-names = "default";
	reset-gpios = <&tlmm 77 GPIO_ACTIVE_LOW>;
	reset-delay-us = <10000>;
	reset-post-delay-us = <50000>;

	rtl8211f_3: ethernet-phy@0 {
		reg = <3>;
	};

	rtl8211f_4: ethernet-phy@1 {
		reg = <4>;
	};
};

&switch {
	status = "okay";
	switch_cpu_bmp =   <0x01>; /* cpu port bitmap */
	switch_lan_bmp =   <0x10>; /* lan port bitmap */
	switch_wan_bmp =   <0x20>; /* wan port bitmap */
	switch_inner_bmp = <0x80>; /*inner port bitmap*/
	switch_mac_mode =  <0x00>; /* mac mode for uniphy instance0*/
	switch_mac_mode1 = <0xff>; /* mac mode for uniphy instance1*/
	switch_mac_mode2 = <0xff>; /* mac mode for uniphy instance2*/

	qcom,port_phyinfo {
		port@3 {
			port_id = <0x04>;
			phy_address = <0x03>;
		};

		port@4 {
			port_id = <0x05>;
			phy_address = <0x04>;
		};
	};
};

&qpic_bam {
	status = "okay";
};

&qpic_nand {
	status = "okay";

	nand@0 {
		reg = <0>;

		nand-ecc-strength = <4>;
		nand-ecc-step-size = <512>;
		nand-bus-width = <8>;
	};
};

&wifi {
	status = "okay";
};
